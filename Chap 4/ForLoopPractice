#either of these ways work
for x in range(1,201):
    print(x*5, end = ", ")
for x in range(5,1001,5):
    print(x, end = ", ")

num = int(input("\nwhen do you want me to stop the loop?"))
num +=1
for x in range(5,num,5):
    print(x, end = ", ")
print()
for x in range(100,0,-1):
    print(str(x),end =", ")

